<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <table class="table">
        <tr>
            <th>ID</th>
            <th>What to do?</th>
            <th>Done?</th>
            <th>Urgent?</th>
        </tr>
        @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model[i].Id</td>
            <td>@Model[i].Title</td>
            <td>
                @Model[i].IsDone
            </td>
            <td>@Model[i].IsUrgent</td>
        </tr>
}



    </table>
</body>
</html>

